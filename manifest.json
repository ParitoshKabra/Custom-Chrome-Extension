{
    "manifest_version": 2,
    "name": "CyBorg",
    "description": "Quick Navigation over Software Developing Websites",
    "version": "1.0.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "unlimitedStorage"
    ]
}